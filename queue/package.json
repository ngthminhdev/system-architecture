    "@nestjs/platform-express": "^10.0.0",
    "@types/jaeger-client": "^3.18.7",
    "axios": "^1.7.2",
    "glossy": "^0.1.7",
    "i": "^0.3.7",
    "jaeger-client": "^3.19.0",
    "kafkajs": "^2.2.3",
    "ngthminhdev-nestjs-kafka": "^0.0.2",
    "npm": "^10.8.1",
